
////       count sorting     ///////////

#include<stdio.h>
void counting(char arr[],int n){
	int i,max,min;
	min=arr[0];
	max=arr[0];
		for(i=0;i<n;i++){
		if(arr[i]>max)
		  max=arr[i];
		if(arr[i]<min)
		  min=arr[i];
	}
	printf("%c",max);
	
	// initializng count array  values to 0;
	int count[max];
		for(i=0;i<=max;i++){
		count[i]=0;
	}
	
	// frequency of element
	for(i=0;i<n;i++){
		count[arr[i]]++;
	}
	
	//printing count values
	printf("frequency of alphabet in aray \n");
		for(i=min;i<=max;i++){
	           printf("[%d]",count[i]);
	}
	
	
	for(i=min+1;i<=max;i++){
	  count[i]=count[i]+count[i-1];
	}

   int b[n];
   for(i=n-1;i>=0;i--){
   	b[--count[arr[i]]]=arr[i];
   }
   for(i=0;i<n;i++)
      arr[i]=b[i];
   // coping array value
   	
}
int main(){
	char arr[50];
	int n,i;
	printf("enetr the total no of alphabet\n");
	scanf("%d",&n);
		printf("enter alphabet\n");
	for(i=0;i<n;i++){
	  scanf("%s",&arr[i]);
     }
	  
	counting(arr,n);

   	puts("\n\nafter sorting\n");
		for(i=0;i<=n-1;i++){
	           printf("[%c]",arr[i]);
	}
}
